{
	"$schema": "https://schemas.wp.org/trunk/block.json",
	"apiVersion": 3,
	"name": "somira/custom-link",
	"version": "1.0.0",
	"title": "Custom Link",
	"category": "text",
	"icon": "admin-links",
	"description": "A customizable link with text and decorative arrow icon.",
	"keywords": ["link", "custom", "arrow", "navigation"],
	"example": {},
	"attributes": {
		"prefixText": {
			"type": "string",
			"default": ""
		},
		"linkText": {
			"type": "string",
			"default": "Place More Blog"
		},
		"linkUrl": {
			"type": "string",
			"default": ""
		}
	},
	"supports": {
		"html": false,
		"align": ["left", "center", "right"],
		"spacing": {
			"margin": true,
			"padding": true
		}
	},
	"textdomain": "somira",
	"editorScript": "file:../dist/index.js",
	"editorStyle": "file:../dist/index.css", 
	"style": "file:../dist/style-index.css",
	"viewScript": "file:../src/view.js"
}